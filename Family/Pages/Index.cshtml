@page
@model IndexModel
@{
   ViewData["Title"] = "Home page";
}

<div>
   <h1>Family Members</h1>
   <ul>
      @foreach (var member in Model.Members) {
      <li>
         <a href="/member?id=@member.Id">
            @member.DisplayName
         </a>
         &nbsp;@member.Dob?.Year
         &nbsp;, @member.FirstName @member.MiddleName @member.LastName
      </li>
      }
   </ul>
</div>